<div class="container">
  <h2>New User</h2>
  <div class="well">
    <%= simple_form_for @user do |f| %>
    <%= f.input :first_name%>
    <%= f.input :last_name %>
    <%= f.input :email%>
    <%= f.input :password %>
    <%= f.input :password_confirmation, label: 'Confirm Password' %>
    <%= f.submit %>
    <% end %>
  </div>

</div>
