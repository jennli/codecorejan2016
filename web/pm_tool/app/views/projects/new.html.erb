<h1>Creating Project</h1>

<div>
  <%= @project.errors.full_messages.join(",") %>
</div>

<div>
  <%= form_for @project do |f| %>
  <div>
    <%= f.label :title  %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.label :due_date %>
   <%= f.date_field :due_date %>
  </div>
  <div>
    <%= f.submit %>
  </div>
  <% end %>
</div>
